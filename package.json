{
  "name": "backend",
  "version": "2.0.0",
  "description": "Backend for patient manager",
  "engines": {
    "node": "10.15.1"
  },
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "start cmd /k nodemon app/index.js && nodemon src --exec babel --presets es2015 -w src/ --out-dir app/",
    "build": "babel src --out-dir dist",
    "start": "set NODE_ENV=production node dist/index.js",
    "serve": "nodemon --exec babel-node src/index.js"
  },
  "keywords": [
    "Management",
    "Patient",
    "RestAPI",
    "Express",
    "MongoDB",
    "CRUD"
  ],
  "author": "Pranav Raut",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "connect-mongodb-session": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.17.1",
    "fs.js": "0.0.1",
    "helmet": "^3.22.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "lz-string": "^1.4.4",
    "moment": "^2.25.3",
